    margin-right: 10px;
}
.product-list .image img {
    border: 1px solid #E7E7E7;
    padding: 3px;
}
.product-list .name {
    margin-bottom: 3px;
}
.product-list .name a {
    color: #0066c0;
    font-weight: normal;
	font-size:17px;
