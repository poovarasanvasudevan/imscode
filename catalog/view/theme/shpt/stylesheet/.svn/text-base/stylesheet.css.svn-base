}
.product-filter .display a {
    font-weight: bold;
}
.product-filter .sort {
    color: gray;
    float: right;
}
.product-filter .limit {
    color: gray;
    float: right;
    margin-left: 15px;
}
.product-compare {
    font-weight: bold;
    margin-bottom: 25px;
    padding-top: 6px;
}
.product-compare a {
    font-weight: bold;
    text-decoration: none;
}
.product-list > div {
    margin-bottom: 15px;
    overflow: auto;
    padding-top: 30px;
}
.product-list .right {
    float: right;
    margin-left: 15px;
}
.product-list > div + div {
    border-top: 1px solid #EEEEEE;
    padding-top: 16px;
}
.product-list .image {
