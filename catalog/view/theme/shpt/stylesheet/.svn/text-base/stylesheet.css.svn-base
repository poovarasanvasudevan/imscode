@import url(//fonts.googleapis.com/css?family=Oswald|Roboto:400,300|Roboto+Condensed:400,300|Open+Sans:400,300,800);
body {
    /*background: url("../image/body_bg.png") repeat scroll 0 0 transparent;*/
    color: #000000;
    font-family: Arial,Helvetica,sans-serif;
    margin: 0;
    padding: 0 0 10px;
}
body, td, th, input, textarea, select, a {
    font-size: 12px;
}
h1, .welcome {
    color: #2F7CC4;
    font-size: 32px;
    font-weight: normal;
    margin-bottom: 30px;
    margin-top: 0;
    text-shadow: 0 0 1px rgba(0, 0, 0, 0.01);
}
h1.heading{
	background-color:#064365;
	color:#fff;
	text-align:center;
	text-transform:uppercase;
	font-size:24px;
	line-height:41px;
}
h2 {
    color: #000000;
    font-size: 16px;
    margin-bottom: 5px;
    margin-top: 0;
}
h3 {
    color: red;
    font-size: 12px;
    margin-bottom: 5px;
    margin-top: 0;
}
p {
    margin-bottom: 20px;
    margin-top: 0;
}
a, a:visited, a b {
    color: #38B0E3;
    cursor: pointer;
    text-decoration: underline;
}
a:hover {
    text-decoration: none;
}
a img {
    border: medium none;
}
form {
    display: inline;
    margin: 0;
    padding: 0;
}
input[type="text"], input[type="password"], textarea {
    background: none repeat scroll 0 0 #F8F8F8;
    border: 1px solid #CCCCCC;
    margin-left: 0;
    margin-right: 0;
    padding: 3px;
}
select {
    background: none repeat scroll 0 0 #F8F8F8;
    border: 1px solid #CCCCCC;
    padding: 2px;
}
label {
    cursor: pointer;
}
#container {
    margin-left: auto;
    margin-right: auto;
    text-align: left;
    width: auto;
}
#column-left {
    float: left;
    width: 212px;
	margin-right:50px;
}
#column-right {
    float: right;
    margin-top: 20px;
    width: 180px;
}
#content {
    min-height: 400px;
    overflow: hidden;
}
.homepage-content {
    border: medium none !important;
    margin-top: 0 !important;
    padding: 0 !important;
}
#column-left + #column-right + #content, #column-left + #content {
    margin-left: 155px;
}
#column-right + #content {
    margin-right: 195px;
}
#header {
	background:url("../image/header-bg.jpg") repeat-x;
    height: 91px;
    z-index: 99;
	border-bottom:3px solid #3b7abd;
}
#header .header{
	width:1000px;
	margin:0 auto;
	position: relative;
	height: 91px;
}
#header #logo {
    position: relative;
	float:left;
	height:60px;
	z-index:102;
}
#header #logo a{
	display:inline-block;
	float:left;
	height:60px;
	position:relative;
	top:15px;
	width:76px;	
}
#language {
    color: #999999;
    left: 320px;
    line-height: 17px;
    position: absolute;
    top: 15px;
    width: 80px;
}
#language img {
    cursor: pointer;
    margin-right: 5px;
}
#currency {
    color: #999999;
    left: 425px;
    line-height: 17px;
    position: absolute;
    top: 15px;
    width: 75px;
}
#currency a {
    border: 1px solid #CCCCCC;
    color: #999999;
    display: inline-block;
    margin-bottom: 2px;
    margin-right: 2px;
    padding: 2px 4px;
    text-decoration: none;
}
#currency a b {
    color: #000000;
    text-decoration: none;
}
#header #cart {
    position: relative;
    float: right;
    margin-top: 12px;
    z-index: 100;
}
#header #cart .c-home{
	background:url("../image/home-icon.png") no-repeat 20px 7px #888;
	width:42px;
	height:25px;
	display:inline-block;
	border-radius:14px 0 0 14px;
	float:left;
}
#cart .c-home a{
	display: inline-block;
    line-height: 25px;
    text-indent: -9000px;
    width: 42px;
}
#header #cart .heading {
    float: right;
	padding:5px 14px 4px;
	border-radius:0 14px 15px 0;
    position: relative;
    z-index: 1;
	background-color:#eee;
}
#header #cart .heading h4 {
    color: #3b7abd;
    font-size: 14px;
	font-weight:normal;
    margin-bottom: 3px;
    margin-top: 0;
	display:inline;
}
#header #cart .heading a {
    color: #3b7abd;
	font-size:14px;
    text-decoration: none;
}
#header #cart .heading a span {
    background: url("../image/arrow-down.png") no-repeat scroll 100% 50% transparent;
    padding-right: 15px;
}
#header #cart .content {
    background: none repeat scroll 0 0 #FFFFFF;
    border-color: #EEEEEE;
    border-radius: 0 0px 7px 7px;
    border-style: solid;
    border-width: 1px;
    box-shadow: 0 2px 2px #DDDDDD;
    clear: both;
	width:300px;
    display: none;
    min-height: 150px;
    padding: 8px;
	float:right;
    position: absolute;
    right: 0px;
	top:25px;
}
#header #cart.active .heading {
    border-radius: 0 14px 0 0;
}
#header #cart.active .content {
    display: block;
}
.mini-cart-info table {
    border-collapse: collapse;
    margin-bottom: 5px;
    width: 100%;
}
.mini-cart-info td {
    border-bottom: 1px solid #EEEEEE;
    color: #000000;
    padding: 10px 5px;
    vertical-align: top;
}
.mini-cart-info .image {
    width: 1px;
}
.mini-cart-info .image img {
    border: 1px solid #EEEEEE;
    text-align: left;
}
.mini-cart-info .name small {
    color: #666666;
}
.mini-cart-info .quantity {
    text-align: right;
}
.mini-cart-info td.total {
    text-align: right;
}
.mini-cart-info .remove {
    text-align: right;
}
.mini-cart-info .remove img {
    cursor: pointer;
}
.mini-cart-total {
    text-align: right;
}
.mini-cart-total table {
    border-collapse: collapse;
    display: inline-block;
    margin-bottom: 5px;
}
.mini-cart-total td {
    color: #000000;
    padding: 4px;
}
#header #cart .checkout {
    clear: both;
    text-align: right;
}
#header #cart .empty {
    padding-top: 50px;
    text-align: center;
}
#header #search {
    position: relative;
    float: right;
    width: auto;
    z-index: 15;
}
#header .button-search {
    background: url("../image/search-btn.png") no-repeat scroll center center #2177d8;
    border-left: 1px solid #CCCCCC;
    cursor: pointer;
    height: 24px;
    right: 0;
    position: absolute;
    width: 24px;
}
#header #search input {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #CCCCCC;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 2px 0 #F0F0F0;
    height: 21px;
    padding: 1px 33px 1px 5px;
    width: auto;
}
#header #welcome {
    color: #999999;
    position: absolute;
    right: 0;
    text-align: right;
    top: 47px;
    width: 298px;
    z-index: 5;
}
#header .links {
    bottom: 3px;
    display: none;
    font-size: 10px;
    font-weight: bold;
    letter-spacing: 0.7px;
    padding-right: 10px;
    position: absolute;
    right: 0;
    text-transform: uppercase;
}
#header .links a {
    color: black;
    display: block;
    float: left;
    font-size: 12px;
    padding: 0 0 0 7px;
    text-decoration: none;
}
#header .links a + a {
    margin-left: 50px;
}
#menu {
    background: transparent;
    height: 52px;
    text-align: center;
    text-transform: uppercase;
	position: relative;
	bottom:-39px;
}
#menu ul {
    display: inline-block;
    list-style: none outside none;
    margin: 0 0 0 15px;
    padding: 0;
	float:left;
}
#menu > ul > li {
    float: left;
    position: relative;
    z-index: 20;
}
#menu > ul > li:hover {
    /*background: none repeat scroll 0 0 #D6E6F6;*/
}
#menu > ul > li > a {
    color: #000000;
    display: block;
    font-size: 13px;
    line-height: 52px;
    bottom: 3px;
    padding: 0 15px;
    text-decoration: none;
    z-index: 6;
}
#menu > ul > li.active_link > a {
	color: #3979B7;
}
#menu > ul > li > a:after {
	content:"";
	position:absolute;
	width:3px;
	height:52px;
	right:0;
	background:url("../image/menu-seperator.png") no-repeat;
}
#menu > ul > li:last-child > a:after { background:none; width:0; }
#menu > ul > li:hover > a {
    color: #3979B7;
}
#menu > ul > li > div {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #FFFFFF;
    border-color: lightgray lightgray #2E7CC7;
    border-image: none;
    border-radius: 0 0 5px 5px;
    border-style: solid;
    border-width: 1px 1px 4px;
    color: #000000;
    display: none;
    position: absolute;
    text-align: left;
    width: 100%;
    z-index: 5;
}
#menu > ul > li:hover > div {
    display: table;
}
#menu > ul > li > div > ul {
    display: table-cell;
    padding: 0;
}
#menu > ul > li ul + ul {
    padding-left: 20px;
}
#menu > ul > li ul > li > a {
    color: #FFFFFF;
    display: block;
    min-width: 120px;
    padding: 4px;
    text-decoration: none;
    white-space: nowrap;
}
#menu > ul > li ul > li > a:hover {
    background: none repeat scroll 0 0 #D6E6F6;
}
#menu > ul > li > div > ul > li > a {
    color: #000000;
}
.breadcrumb {
    color: #000000;
    margin-bottom: 10px;
    position: relative;
    text-align: right;
}
.breadcrumb a {
    color: #000000;
    font-weight: bold;
    text-decoration: none;
}
.success, .warning, .attention, .information {
    border-radius: 5px 5px 5px 5px;
    color: #555555;
    margin-bottom: 15px;
    padding: 10px 10px 10px 33px;
}
.success {
    background: url("../image/success.png") no-repeat scroll 10px center #EAF7D9;
    border: 1px solid #BBDF8D;
    border-radius: 5px 5px 5px 5px;
}
.warning {
    background: url("../image/warning.png") no-repeat scroll 10px center #FFD1D1;
    border: 1px solid #F8ACAC;
    border-radius: 5px 5px 5px 5px;
}
.attention {
    background: url("../image/attention.png") no-repeat scroll 10px center #FFF5CC;
    border: 1px solid #F2DD8C;
    border-radius: 5px 5px 5px 5px;
}
.success .close, .warning .close, .attention .close, .information .close {
    cursor: pointer;
    float: right;
    padding-right: 4px;
    padding-top: 4px;
}
.required {
    color: #FF0000;
    font-weight: bold;
}
.error {
    color: #FF0000;
    display: block;
}
.help {
    color: #999999;
    display: block;
    font-family: Verdana,Geneva,sans-serif;
    font-size: 10px;
    font-weight: normal;
}
table.form {
    border-collapse: collapse;
    margin-bottom: 20px;
    width: 100%;
}
table.form tr td:first-child {
    width: 150px;
}
table.form > * > * > td {
    color: #000000;
}
table.form td {
    padding: 4px;
}
input.large-field, select.large-field {
    width: 300px;
}
table.list {
    border-collapse: collapse;
    border-left: 1px solid #DDDDDD;
    border-top: 1px solid #DDDDDD;
    margin-bottom: 20px;
    width: 100%;
}
table.list td {
    border-bottom: 1px solid #DDDDDD;
    border-right: 1px solid #DDDDDD;
}
table.list thead td {
    background-color: #EFEFEF;
    padding: 0 5px;
}
table.list thead td a, .list thead td {
    color: #222222;
    font-weight: bold;
    text-decoration: none;
}
table.list tbody td {
    padding: 0 5px;
    vertical-align: top;
}
table.list .left {
    padding: 7px;
    text-align: left;
}
table.list .right {
    padding: 7px;
    text-align: right;
}
table.list .center {
    padding: 7px;
    text-align: center;
}
table.radio {
    border-collapse: collapse;
    width: 100%;
}
table.radio td {
    padding: 5px;
    vertical-align: top;
}
table.radio td label {
    display: block;
}
table.radio tr td:first-child {
    width: 1px;
}
table.radio tr td:first-child input {
    margin-top: 1px;
}
table.radio tr.highlight:hover td {
    background: none repeat scroll 0 0 #F1FFDD;
    cursor: pointer;
}
.pagination {
    border-top: 1px solid #EEEEEE;
    display: inline-block;
    margin-bottom: 10px;
    padding-top: 8px;
    width: 100%;
}
.pagination .links {
    float: left;
}
.pagination .links a {
    border: 1px solid #EEEEEE;
    color: #A3A3A3;
    display: inline-block;
    padding: 4px 10px;
    text-decoration: none;
}
.pagination .links b {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #269BC6;
    color: #269BC6;
    display: inline-block;
    font-weight: normal;
    padding: 4px 10px;
    text-decoration: none;
}
.pagination .results {
    float: right;
    padding-top: 3px;
}
a.button, input.button {
    /*background: none repeat scroll 0 0 #2F7EC8;*/
	background: -webkit-linear-gradient(top, #f8de9c, #f1c340); /* For Safari 5.1 to 6.0 */
  	background: -o-linear-gradient(top, #f8de9c, #f1c340); /* For Opera 11.1 to 12.0 */
  	background: -moz-linear-gradient(top, #f8de9c, #f1c340); /* For Firefox 3.6 to 15 */
  	background: linear-gradient(top, #f8de9c, #f1c340); /* Standard syntax */
	border:1px solid #ccaa50!important;	
    border-radius: 3px 3px 3px 3px;
	box-shadow:0 1px 0 rgba(255,255,255,0.5) inset;
    color: #222;
    cursor: pointer;
    font-family: arial;
    font-size: 11px;
    font-weight: bold;
    line-height: 12px;
    text-transform: uppercase;
    width: auto;
}
a.button {
    display: inline-block;
    padding: 10px 12px;
    text-decoration: none;
	margin-top:10px;
}
input.button {
    border: 0 none;
    line-height: 12px;
    margin-top:10px;
    padding: 10px 12px;
}
.buttons {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #EEEEEE;
    margin-bottom: 20px;
    overflow: auto;
    padding: 6px;
}
.buttons .left {
    float: left;
    text-align: left;
}
.buttons .right {
    float: right;
    text-align: right;
}
.buttons .center {
    margin-left: auto;
    margin-right: auto;
    text-align: center;
}
.htabs {
    border-bottom: 1px solid #DDDDDD;
    height: 30px;
    line-height: 16px;
}
.htabs a.selected {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #FFFFFF;
    border-color: #DDDDDD #DDDDDD -moz-use-text-color;
    border-image: none;
    border-style: solid solid none;
    border-width: 1px 1px medium;
    padding-bottom: 7px;
}
.htabs a {
    border: medium none;
    color: #2F7DC8;
    display: none;
    float: left;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 13px;
    font-weight: bold;
    margin-right: 2px;
    padding: 7px 15px 6px;
    text-align: center;
    text-decoration: none;
}
.tab-content {
    margin-bottom: 20px;
    overflow: auto;
    padding: 20px;
    z-index: 2;
}
.box {
    margin-bottom: 20px;
	/*position:relative;
	top:-85px;*/
}
.box .box-heading {
    /*background: url("../image/featured_line.png") repeat-x scroll center center transparent;*/
    color: #555;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 18px;
    font-weight: bold;
    line-height: 14px;
    padding: 8px 10px 15px 0;
	border-bottom:1px solid #333;
}
.box .box-content {
    background: none repeat scroll 0 0 #FFFFFF;
    border-radius: 0 0 7px 7px;
    font-weight: bold;
    padding: 10px 10px 10px 30px;
}
.box-product {
    overflow: auto;
    width: 100%;
}
.box-product > div {
    border-right: 1px solid lightgray;
    display: inline-block;
    margin-bottom: 20px;
    padding: 25px;
    text-align: center;
    vertical-align: middle;
    width: 130px;
}
#column-left + #column-right + #content .box-product > div {
    width: 119px;
}
.box-product .image {
    display: block;
    margin-bottom: 0;
}
.box-product .image img {
    padding: 3px;
}
.box-product .name a {
    color: #333333;
    display: block;
    font-weight: bold;
    margin-bottom: 4px;
    text-decoration: none;
}
.box-product .price {
    color: #666666;
    display: block;
    font-weight: bold;
    margin-bottom: 4px;
    padding-top: 10px;
}
.box-product .price-old {
    color: #FF0000;
    text-decoration: line-through;
}
.box-product .price-new {
    font-weight: bold;
}
.box-product .rating {
    display: block;
    margin-bottom: 4px;
}
.box-category {
    margin-top: -5px;
}
.box-category ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}
.box-category > ul > li {
    padding: 8px 8px 8px 0;
	position:relative;
}
.box-category > ul > li:before {
    content: "";
	position:absolute;
	background:url("../image/left-bar-bullet.jpg")no-repeat;
    font-size: 8px;
	width:2px;
	height:3px;
    font-weight: normal;
    padding-bottom: 4px;
    vertical-align: middle;
}
.box-category > ul > li + li {
    border-top: 1px dotted #EEEEEE;
}
.box-category > ul > li > a {
    color: #333333;
    text-decoration: none;
    text-transform: capitalize;
}
.box-category > ul > li ul {
    display: none;
}
.box-category > ul > li a.active {
    color: #2F7CC4;
    font-weight: bold;
}
.box-category > ul > li a.active + ul {
    display: block;
}
.box-category > ul > li ul > li {
    padding: 5px 5px 0 10px;
}
.box-category > ul > li ul > li > a {
    color: #000000;
    display: block;
    text-decoration: none;
    text-transform: capitalize;
}
.box-category > ul > li ul > li > a.active {
    font-weight: bold;
}
#content .content {
    border: 1px solid #EEEEEE;
    margin-bottom: 20px;
    overflow: auto;
    padding: 10px;
}
#content .content .left {
    float: left;
    width: 49%;
}
#content .content .right {
    float: right;
    width: 49%;
}
.category-info {
    margin-bottom: 20px;
    overflow: auto;
}
.category-info .image {
    border: 1px solid #E7E7E7;
    float: left;
    margin-right: 15px;
    padding: 5px;
}
.category-list {
    margin-bottom: 20px;
    overflow: auto;
}
.category-list ul {
    float: left;
    width: 18%;
}
.manufacturer-list {
    border: 1px solid #DBDEE1;
    margin-bottom: 20px;
    overflow: auto;
    padding: 5px;
}
.manufacturer-heading {
    background: none repeat scroll 0 0 #F8F8F8;
    font-size: 15px;
