}
.product-list .rating {
    color: #7B7B7B;
}
.product-list .price {
    color: #b12704 !important;
    font-size: 14px;
	font-weight:bold;
}
.product-list .price-old {
    color: #FF0000;
    text-decoration: line-through;
}
.product-list .price-new {
    font-weight: bold;
